    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.5",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.11",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "class-transformer": "^0.5.1",
