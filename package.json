    "file-stream-rotator": "^1.0.0",
    "helmet": "^7.0.0",
    "lodash.merge": "^4.6.2",
    "mongodb": "^5.8.1",
    "openid-client": "^5.5.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "radash": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
